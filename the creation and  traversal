#include <iostream>
using namespace std;
struct node {
 int data;
 node*left;
 node*right;
  node(int val){
      data=val;
      left=nullptr;
      right=nullptr;
  }
};
  void preorder(node*root){
       if(root){
           cout<<root->data<<" ";
           preorder(root->left);
           preorder(root->right);
           
       }
   }
void inorder(node*root){
    if(root){
        inorder(root->left);
        cout<<root->data<<" ";
        inorder(root->right);
    }
}
void postorder(node*root){
    if(root){
        postorder(root->left);
        postorder(root->right);
        cout<<root->data<<" ";
    }
}
 int main(){
     node*root= new node(10);
     root->left= new node(11);
     root->right= new node(12);
     root->left->left= new node (13);
     root->left->right= new node(14);
     root-> right->left= new node(15);
     root-> right->right= new node(16);
     cout<<"preorder";
     preorder(root);
     cout<<endl;
     cout<<"inorder"; 
     inorder(root);
     cout<<endl; 
     cout<<"postorder";
     postorder(root);
     cout<<endl;
 }
 
   
