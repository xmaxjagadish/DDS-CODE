#include <stdio.h>
#include <stdlib.h>
int main() {
  int n;
  printf("enter the size of array");
  scanf("%d",&n);
  int *array=(int*) malloc ( n*sizeof (int));
  for(int i=0; i<n; i++){
      printf("enter the value at array [%d] index",i);
      scanf("%d",&array[i]);
      }
for(int j=0; j<n; j++){
      printf(" %d",array [j]);
    }
  
    return 0;
}
