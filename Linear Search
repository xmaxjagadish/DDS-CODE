#include<iostream>
using namespace std;
int main(){
    int arr[]={10,3,5,9,8};
    int n=sizeof(arr)/sizeof(arr[0]);
    int index=-1;
    int key=9;
    for(int i=0;i<n;i++){
        if (arr[i]==key){
            index=i;
            break;
        }
    }
    if(index==-1){
        cout<<"key not found";
    }
    else{
        cout<<"found at index"<<index;
    }
}
