#include <iostream>
using namespace std;
int main() {
   int arr[]={10,20,40,80,100};          // 1. Missing semicolon here
   int n=sizeof(arr)/sizeof(arr[0]);     // 2. Wrong operator (?) used, should be '/' (division)
   int low =0; 
   int index=-1;
   int key=10;     // 3. Missing 'int' before 'key'; multiple statements in one line without proper syntax
   int high= n-1;
   while(low<=high){
       int mid=(low+high);            // 4. 'z' is undefined, should be 2
       if (arr[mid]==key){               // 5. Array access uses [] not ()
           index=mid;
           break;
       }
   else if (arr[mid]<key){              // 6. Same as above: use [] instead of ()
       low=mid+1;
   }
   else{
       high=mid-1;
   }
   }
   if(index==-1){                     // 8. 'id' should be 'if'
       cout<<"not found";
   }
   else{
       cout<<"found at index"<<index;
   }
    return 0;
}
